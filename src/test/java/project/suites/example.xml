<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="regression" parallel="tests" thread-count="3">
    <groups>
        <run>
            <include name="regression" />
        </run>
    </groups>
    <test name="Validate Currency Rate">
        <classes>
            <class name="project.cases.ValidateCurrencyRateTest" />
        </classes>
    </test>
    <test name="Wrong Creds">
        <classes>
            <class name="project.cases.WrongCredsTest" />
        </classes>
    </test>
    <test name="DB test">
        <classes>
            <class name="project.cases.DbTest" />
        </classes>
    </test>
</suite>